{
  "manifest_version": 3,
  "name": "My Extension",
  "description": "Starter template built with ExtensionKit.io",
  "version": "1.0.0",
  "icons": {
    "128": "icons/icon.png"
  },
  "action": {
    "default_icon": {
      "32": "icons/icon.png"
    },
    "default_popup": "index.html",
    "default_title": "Open Popup"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+O",
        "mac": "MacCtrl+Shift+O"
      },
      "description": "Toggle Popup"
    }
  },
  "permissions": ["activeTab", "scripting"]
}
